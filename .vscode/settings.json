{
  "eslint.validate": [
    "vue",
    "html",
    "yaml",
    "json",
    "jsonc",
    "json5",
    "react",
    "astro",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.experimental.useFlatConfig": true,
  "cSpell.words": [
    "antfu",
    "bumpp",
    "changelogithub",
    "ependencies",
    "eqeqeq",
    "iife",
    "jsdelivr",
    "npmrc",
    "nuxt",
    "pnpm",
    "stroustrup",
    "sxzz",
    "tsup",
    "vitepress",
    "vuepress"
  ]
}
