{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never"
  },
  "eslint.validate": [
    "vue",
    "xml",
    "html",
    "yaml",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.useGitignore": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "antfu",
    "attributify",
    "bumpp",
    "changelogithub",
    "dprint",
    "ependencies",
    "eqeqeq",
    "esbenp",
    "iife",
    "jiti",
    "jsdelivr",
    "lockb",
    "noalias",
    "nocheck",
    "nocollapse",
    "nocompile",
    "noinline",
    "npmrc",
    "nuxt",
    "pinia",
    "pnpm",
    "shadcn",
    "slidev",
    "stroustrup",
    "sxzz",
    "tauri",
    "tseslint",
    "tsup",
    "unplugin",
    "vitepress",
    "vuepress"
  ]
}
