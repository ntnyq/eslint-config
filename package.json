{
  "name": "@ntnyq/eslint-config",
  "type": "module",
  "version": "5.8.0",
  "packageManager": "pnpm@10.26.2",
  "description": "An opinionated ESLint config preset of ntnyq",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-config.ntnyq.com",
  "repository": "ntnyq/eslint-config",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-config/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "build": "run-s types:generate lib:build",
    "dev": "run-s types:generate lib:dev",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "inspector:build": "run-s build inspector:generate",
    "inspector:generate": "pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "lib:build": "tsdown",
    "lib:dev": "tsdown --watch",
    "lint": "eslint --cache",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s build lint test typecheck",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsgo --noEmit",
    "types:generate": "tsx scripts/generateType.ts"
  },
  "peerDependencies": {
    "@html-eslint/eslint-plugin": "^0.50.0",
    "@html-eslint/parser": "^0.50.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.20.0",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-eslint-plugin": "^7.2.0",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-pnpm": "^1.4.1",
    "eslint-plugin-svelte": "^3.13.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "svelte": "^5.45.6",
    "svelte-eslint-parser": "^1.4.1"
  },
  "peerDependenciesMeta": {
    "@html-eslint/eslint-plugin": {
      "optional": true
    },
    "@html-eslint/parser": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-pnpm": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-unused-imports": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@unocss/eslint-plugin": "^66.5.11",
    "@vitest/eslint-plugin": "^1.6.4",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-merge-processors": "^2.0.0",
    "eslint-parser-plain": "^0.1.1",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.4.0",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-depend": "^1.4.0",
    "eslint-plugin-github-action": "^0.1.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-ntnyq": "^0.12.0",
    "eslint-plugin-perfectionist": "^5.0.0",
    "eslint-plugin-pinia": "^0.4.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-svgo": "^0.11.3",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-plugin-yml": "^1.19.1",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^2.4.2",
    "local-pkg": "^1.1.2",
    "toml-eslint-parser": "^0.10.1",
    "typescript-eslint": "^8.50.1",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^1.3.2"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^25.0.3",
    "@typescript-eslint/types": "^8.50.1",
    "@typescript-eslint/utils": "^8.50.1",
    "@typescript/native-preview": "7.0.0-dev.20251221.1",
    "bumpp": "^10.3.2",
    "consola": "^3.4.2",
    "eslint": "^9.39.2",
    "eslint-typegen": "^2.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.7.4",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.18.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "uncase": "^0.2.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": "^20.11.0 || >=21.1.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
