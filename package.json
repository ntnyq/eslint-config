{
  "name": "@ntnyq/eslint-config",
  "type": "module",
  "version": "5.0.0-beta.1",
  "packageManager": "pnpm@10.11.0",
  "description": "An opinionated ESLint config preset of ntnyq",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-config.ntnyq.com",
  "repository": "ntnyq/eslint-config",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-config/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "scripts": {
    "build": "run-s generate:type build:lib",
    "build:inspector": "pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "build:lib": "tsdown",
    "dev": "tsdown --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "generate:site": "run-s generate:type build:lib build:inspector",
    "generate:type": "tsx scripts/generateType.ts",
    "lint": "eslint --cache",
    "prepare": "husky",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s build lint test typecheck",
    "release:publish": "pnpm publish --tag next",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@html-eslint/eslint-plugin": "^0.40.3",
    "@html-eslint/parser": "^0.40.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.20.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "peerDependenciesMeta": {
    "@html-eslint/eslint-plugin": {
      "optional": true
    },
    "@html-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-unused-imports": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.10.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.27.0",
    "@eslint/markdown": "^6.4.0",
    "@unocss/eslint-plugin": "^66.1.2",
    "@vitest/eslint-plugin": "^1.2.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-merge-processors": "^2.0.0",
    "eslint-parser-plain": "^0.1.1",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.2.0",
    "eslint-plugin-de-morgan": "^1.2.1",
    "eslint-plugin-depend": "^1.2.0",
    "eslint-plugin-github-action": "^0.0.16",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-ntnyq": "^0.11.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-pnpm": "^0.3.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-svgo": "^0.8.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "eslint-plugin-vue": "^10.1.0",
    "eslint-plugin-yml": "^1.18.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.1.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "prettier": "^3.5.3",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.32.1",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.15.18",
    "bumpp": "^10.1.1",
    "eslint": "^9.27.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-typegen": "^2.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.2",
    "tinyglobby": "^0.2.13",
    "tsdown": "^0.11.11",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "uncase": "^0.1.0",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
