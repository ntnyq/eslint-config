{
  "name": "@ntnyq/eslint-config",
  "type": "module",
  "version": "4.0.0",
  "packageManager": "pnpm@10.6.1",
  "description": "An opinionated ESLint config preset of ntnyq",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-config.ntnyq.com",
  "repository": "ntnyq/eslint-config",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-config/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s generate:type build:lib",
    "build:inspector": "pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "build:lib": "unbuild",
    "dev": "unbuild --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "generate:site": "run-s generate:type build:lib build:inspector",
    "generate:type": "tsx scripts/generateType.ts",
    "lint": "eslint --cache",
    "prepare": "husky",
    "release": "run-s release:check release:version",
    "release:check": "run-s build lint typecheck",
    "release:version": "bumpp && npm publish --tag next",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.20.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-unused-imports": "^4.1.4"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-unused-imports": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.10.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.22.0",
    "@eslint/markdown": "^6.3.0",
    "@unocss/eslint-plugin": "^66.1.0-beta.3",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.1.0",
    "eslint-plugin-de-morgan": "^1.2.0",
    "eslint-plugin-depend": "^0.12.0",
    "eslint-plugin-github-action": "^0.0.14",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-ntnyq": "^0.10.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-pnpm": "^0.1.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-svgo": "^0.6.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "prettier": "^3.5.3",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.26.0",
    "vue-eslint-parser": "^10.1.1",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.10",
    "bumpp": "^10.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-typegen": "^2.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "tinyglobby": "^0.2.12",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "uncase": "^0.0.4",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "resolutions": {
    "@shikijs/core": "^3.1.0",
    "@shikijs/markdown-it": "^3.1.0",
    "@shikijs/rehype": "^3.1.0",
    "@shikijs/transformers": "^3.1.0",
    "@shikijs/types": "^3.1.0",
    "esbuild": "^0.25.0",
    "shiki": "^3.1.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
