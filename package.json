{
  "name": "@ntnyq/eslint-config",
  "type": "module",
  "version": "5.6.0",
  "packageManager": "pnpm@10.17.1",
  "description": "An opinionated ESLint config preset of ntnyq",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-config.ntnyq.com",
  "repository": "ntnyq/eslint-config",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-config/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "build": "run-s types:generate lib:build",
    "dev": "run-s types:generate lib:dev",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "inspector:build": "run-s build inspector:generate",
    "inspector:generate": "pnpm dlx @eslint/config-inspector build --config eslint-inspector.config.ts",
    "lib:build": "tsdown",
    "lib:dev": "tsdown --watch",
    "lint": "eslint --cache",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s build lint test typecheck",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "types:generate": "tsx scripts/generateType.ts"
  },
  "peerDependencies": {
    "@html-eslint/eslint-plugin": "^0.46.2",
    "@html-eslint/parser": "^0.46.2",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.20.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-pnpm": "^1.1.1",
    "eslint-plugin-svelte": "^3.12.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "svelte": "^5.39.3",
    "svelte-eslint-parser": "^1.3.3"
  },
  "peerDependenciesMeta": {
    "@html-eslint/eslint-plugin": {
      "optional": true
    },
    "@html-eslint/parser": {
      "optional": true
    },
    "astro-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-format": {
      "optional": true
    },
    "eslint-plugin-pnpm": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-unused-imports": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.36.0",
    "@eslint/markdown": "^7.3.0",
    "@unocss/eslint-plugin": "^66.5.2",
    "@vitest/eslint-plugin": "^1.3.13",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-merge-processors": "^2.0.0",
    "eslint-parser-plain": "^0.1.1",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-command": "^3.3.1",
    "eslint-plugin-de-morgan": "^2.0.0",
    "eslint-plugin-depend": "^1.3.1",
    "eslint-plugin-github-action": "^0.0.16",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^60.7.0",
    "eslint-plugin-jsonc": "^2.20.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-ntnyq": "^0.12.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-svgo": "^0.11.3",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-vue": "^10.5.0",
    "eslint-plugin-yml": "^1.18.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "globals": "^16.4.0",
    "jsonc-eslint-parser": "^2.4.1",
    "local-pkg": "^1.1.2",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.45.0",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.6.1",
    "@typescript-eslint/types": "^8.45.0",
    "@typescript-eslint/utils": "^8.45.0",
    "bumpp": "^10.2.3",
    "consola": "^3.4.2",
    "eslint": "^9.36.0",
    "eslint-typegen": "^2.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.15.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "uncase": "^0.2.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^20.11.0 || >=21.1.0"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,vue,svg,yml,yaml,toml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
