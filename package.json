{
  "name": "@ntnyq/eslint-config",
  "type": "module",
  "version": "3.12.3",
  "packageManager": "pnpm@9.15.5",
  "description": "An opinionated ESLint config preset of ntnyq",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/eslint-config",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run generate:type && tsup",
    "dev": "tsup --watch src",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "generate:site": "tsx scripts/generateSite.ts",
    "generate:type": "tsx scripts/generateType.ts",
    "lint": "eslint .",
    "prepare": "husky",
    "release": "run-s release:check release:version",
    "release:check": "run-s build lint typecheck",
    "release:version": "bumpp && npm publish --tag next",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "eslint": "^9.5.0",
    "eslint-plugin-es-x": "^8.4.1",
    "eslint-plugin-eslint-plugin": "^6.4.0"
  },
  "peerDependenciesMeta": {
    "@stylistic/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-es-x": {
      "optional": true
    },
    "eslint-plugin-eslint-plugin": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.9.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.19.0",
    "@eslint/markdown": "^6.2.2",
    "@unocss/eslint-plugin": "^65.4.3",
    "@vitest/eslint-plugin": "^1.1.25",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-merge-processors": "^1.0.0",
    "eslint-plugin-antfu": "^3.0.0",
    "eslint-plugin-command": "^3.0.0",
    "eslint-plugin-depend": "^0.12.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-github-action": "^0.0.13",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-ntnyq": "^0.9.2",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-pinia": "^0.4.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-svgo": "^0.4.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yml": "^1.16.0",
    "eslint-processor-vue-blocks": "^1.0.0",
    "globals": "^15.14.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.0.0",
    "prettier": "^3.4.2",
    "toml-eslint-parser": "^0.10.0",
    "typescript-eslint": "^8.22.0",
    "vue-eslint-parser": "^9.4.3",
    "yaml-eslint-parser": "^1.2.3"
  },
  "devDependencies": {
    "@ntnyq/prettier-config": "^1.22.1",
    "@types/node": "^22.13.0",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "eslint-typegen": "^1.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4",
    "zx": "^8.3.2"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vitest/eslint-plugin": "patches/@vitest__eslint-plugin.patch"
    }
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,svg,json,jsonc,md,yaml,yml}": "eslint --fix",
    "*.css": "prettier --write"
  }
}
